<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.ginie.superloop</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/zsh</string>
    <string>-c</string>
    <string>bash ~/GinieSystem/Scripts/g.force.loop.sh >> ~/GinieSystem/Logs/loop_autostart.log 2>&1</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>
  <key>StandardOutPath</key>
  <string>/tmp/ginie_autostart.log</string>
  <key>StandardErrorPath</key>
  <string>/tmp/ginie_autostart_error.log</string>
</dict>
</plist>
