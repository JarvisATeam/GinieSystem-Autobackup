<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.ginie.vaultloader</string>
  <key>ProgramArguments</key>
  <array>
    <string>/opt/homebrew/bin/pwsh</string>
    <string>-Command</string>
    <string>Invoke-Expression -Command (Get-Content "$HOME/Library/Mobile Documents/com~apple~CloudDocs/GinieSystem/Vault/keys/vault_loader.ps1" -Raw)</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardOutPath</key>
  <string>$HOME/GinieSystem/Logs/vault_autoload.log</string>
  <key>StandardErrorPath</key>
  <string>$HOME/GinieSystem/Logs/vault_autoload.err</string>
</dict>
</plist>
